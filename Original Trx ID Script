Select TrxTransactionSaleItem.TransactionKey, TrxTransaction.TransactionId As [Invoice ID], TrxTransactionSaleItem.OriginalDocumentKey, TrxTransactionSaleItem.TransactionItemKey, TrxTransaction1.TransactionId As [Quotation ID] From TrxTransactionSaleItem Inner Join TrxTransaction On TrxTransactionSaleItem.OriginalDocumentKey = TrxTransaction.TransactionKey Inner Join TrxTransaction TrxTransaction1 On TrxTransactionSaleItem.TransactionKey = TrxTransaction1.TransactionKey Where TrxTransactionSaleItem.TransactionKey = @TransactionKey
